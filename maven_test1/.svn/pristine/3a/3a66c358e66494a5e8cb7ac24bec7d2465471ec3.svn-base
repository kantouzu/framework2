    public static BigDecimal convert(BigDecimal db) {
        double d = isEmpty(db) ? 0.0 : db.doubleValue();
        long l1 = Math.round(d * 100); // 四舍五入
        double ret = l1 / 100.0;
        return new BigDecimal(ret);
    }
