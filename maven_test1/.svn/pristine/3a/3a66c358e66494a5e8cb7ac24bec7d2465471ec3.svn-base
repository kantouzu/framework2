     */
    public static String judgmentRole(String Value) {
        String roles = "";
        if (Value == AuditLevel.三级审.getValue()) {
            roles = UserTypeCode.三级审批人员.getValue();
        } else if (Value == AuditLevel.二级初审.getValue()) {
            roles = UserTypeCode.二级审批人员.getValue() + "," + UserTypeCode.初审人员.getValue();
        } else if (Value == AuditLevel.二级复审.getValue()) {
            roles = UserTypeCode.二级审批人员.getValue() + "," + UserTypeCode.复审人员.getValue();
        } else if (Value == AuditLevel.二级终审.getValue()) {
            roles = UserTypeCode.二级审批人员.getValue() + "," + UserTypeCode.终审人员.getValue();
        } else if (Value == AuditLevel.股份审核.getValue()) {
            roles = UserTypeCode.股份审批人员.getValue();
        } else if (Value == AuditLevel.集团初审.getValue()) {
            roles = UserTypeCode.集团审批人员.getValue();
        } else if (Value == AuditLevel.集团复审.getValue()) {
            roles = UserTypeCode.集团审批人员.getValue();
        } else if (Value == AuditLevel.集团终审.getValue()) {
            roles = UserTypeCode.集团审批人员.getValue();
        }
        return roles;

    }

    public Double getDoubleByThousand(String str) {
        Double d = null;
        try {
            d = Double.valueOf(NumberFormat.getInstance(Locale.getDefault()).parse(str).doubleValue());
            return d;
        } catch (ParseException e) {
            e.printStackTrace();
            return d;
        }
    }

    public static String getInCondition(String str) {
        String result = "";
        if (!BaseUtil.isEmpty(str)) {
            String[] strTemp = str.split(",");
            for (String string : strTemp) {
                result = "'" + string + "',";
            }
            result = result.substring(0, result.length() - 1);
        }
        return result;
    }

