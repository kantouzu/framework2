    </if>
    'true' as QUERYID,
		${@com.cisdi.base.util.BaseUtil@transEnumValueToName("is_ignore_pre", "YesOrNo")} isIgnorePreName,
		${@com.cisdi.base.util.BaseUtil@transEnumValueToName("deleteflag", "YesOrNo")} deleteflagName,
		${@com.cisdi.base.util.BaseUtil@transEnumValueToName("is_root", "YesOrNo")} isRootName,
    <include refid="Base_Column_List" />
    from menu
    <if test="_parameter != null">
      <include refid="Example_Where_Clause" />
    </if>
    <if test="orderByClause != null" >
${orderByClause}    </if>
    <if test="limitStart != null">
      limit ${limitStart} , ${limitEnd}
    </if>
  </select>   
  
  <select id="selectTreeMenuByCondition" parameterType="java.util.HashMap" resultMap="treeMenu">
    select
    <include refid="Base_Column_List" />,
    (select count(id) from menu where parent_id = m.id) childrenNum
    from menu m where deleteflag = 0
    and FIND_IN_SET(m.id, getChildLst(
    		<if test="rootId != null">
    			#{rootId}
    		</if>
    		<if test="rootId == null">
	    		(select min(id) from menu where deleteflag = 0)
    		</if>
    	)
   	)
   	and m.id in (
   		select m.id from menu m
   		left join role_rel_menu rel on rel.menu_id = m.id
   		left join role_info r on r.role_code = rel.role_code
   		left join user_rel_role urr on urr.role_code = r.role_code
   		where m.deleteflag = '0'
	    <if test="userId != null">
	      and urr.user_id = #{userId}
	    </if>
   	)
   	<if test="parentId != null">
    and FIND_IN_SET(m.id, getChildLst(#{parentId}))
   	or m.id in ((select min(id) from menu where deleteflag = 0))
    </if>
    <!-- and (m.is_root is null or m.is_root = '0') -->
    order by order_num 
  </select>  
  
  <select id="selectBusinessTreeMenuByCondition" parameterType="java.util.HashMap" resultMap="treeMenu">
    select
    <include refid="Base_Column_List" />,
    (select count(id) from menu where parent_id = m.id) childrenNum
    from menu m where deleteflag = 0
    and FIND_IN_SET(m.id, getChildLst(
    		<if test="rootId != null">
    			#{rootId}
    		</if>
    		<if test="rootId == null">
	    		(select min(id) from menu where deleteflag = 0)
    		</if>
    	)
   	)
   	and m.id in (
   		select m.id from menu m
   		left join role_rel_menu rel on rel.menu_id = m.id
   		left join userdepart_rel_role urr on urr.role_code = rel.role_code
   		left join user_rel_depart urd on urd.id = urr.userdepart_id
   		where m.deleteflag = '0'
	    <if test="userId != null">
	      and urd.user_id = #{userId}
	    </if>
	    <if test="departId != null">
	      and urd.depart_id = #{departId}
	    </if>
   	)
   	<if test="parentId != null">
    and FIND_IN_SET(m.id, getChildLst(#{parentId}))
   	or m.id in ((select min(id) from menu where deleteflag = 0))
    </if>
    <!-- and (m.is_root is null or m.is_root = '0') -->
    order by order_num 
  </select>  
  
