    </if>
    'true' as QUERYID,
	${@com.cisdi.base.util.BaseUtil@transEnumValueToName("locationInfo.deleteflag", "DeleteFlag")} deleteflagName,
    <include refid="Base_Column_List" />
    from location_info locationInfo
    <include refid="Table_Join_Clause" />
