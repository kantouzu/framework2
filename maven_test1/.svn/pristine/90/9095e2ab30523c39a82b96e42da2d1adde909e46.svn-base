    </if>
  </select>    
  
  <select id="selectTreeLocByCondition" parameterType="java.util.HashMap" resultMap="treeDic">
    select
    <include refid="Base_Column_List" />,
    (select count(id) from location_info d where d.parent_id = locationInfo.id) childrenNum
    from location_info locationInfo where locationInfo.deleteflag = 0
    and FIND_IN_SET(locationInfo.id, getChildLstLoc(
    		<if test="rootId != null">
    			#{rootId}
    		</if>
    		<if test="rootId == null">
	    		(select min(id) from location_info where deleteflag = 0)
    		</if>
    	)
   	)
   	<if test="name != null">
    and locationInfo.name like #{name}
    </if>
    <!-- and (m.is_root is null or m.is_root = '0') -->
    order by locationInfo.id 
  </select>
  
  <select id="selectParentTreeLocByCondition" parameterType="java.util.HashMap" resultMap="treeDic">
    select
    <include refid="Base_Column_List" />,
    (select count(id) from location_info d where d.parent_id = locationInfo.id) childrenNum
    from location_info locationInfo where locationInfo.deleteflag = 0
    and FIND_IN_SET(id,getParentListLoc(
